{
  "name": "gandalf",
  "version": "0.0.1",
  "description": "sql client",
  "main": "app.js",
  "dependencies": {
    "JSONStream": "^0.10.0",
    "codemirror": "^5.25.2",
    "electron": "^1.6.2",
    "fuzzy": "^0.1.0",
    "mithril": "^0.1.25",
    "node-jt400": "1.3.0",
    "q": "~1.0.0",
    "sql-parser": "git+https://github.com/jakobrun/sql-parser"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "~4.1.3",
    "chai": "^1.10.0",
    "electron-packager": "^8.6.0",
    "electron-rebuild": "^1.5.7",
    "eslint": "~1.4.3",
    "faker": "^2.1.1",
    "gulp": "^3.8.10",
    "gulp-less": "^1.3.6",
    "husky": "^0.13.3",
    "mocha": "^2.0.1",
    "rimraf": "^2.2.8"
  },
  "repository": {
    "type": "git",
    "url": "gh:jakobrun/gandalf.git"
